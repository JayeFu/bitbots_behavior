<?xml version="1.0"?>
<package>
  <name>bitbots_head_behaviour</name>
  <version>0.1.1</version>
  <description>The bitbots_head_behaviour package is used to control the behaviour of the head.
      The head searches for balls, goals and lines and tracks them. The head behaviour can be 
      controlled by the /head_duty topic and publishes the head motion in the /head_motor_goals topic.
  </description>
  <maintainer email="7engelke@informatik.uni-hamburg.de">Timon Engelke</maintainer>
  <author email="info@bit-bots.de">Hamburg Bit-Bots</author>
  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>std_msgs</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>humanoid_league_msgs</build_depend>
  <build_depend>bitbots_stackmachine</build_depend>
  <build_depend>bitbots_body_behaviour</build_depend>

  <run_depend>std_msgs</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>humanoid_league_msgs</run_depend>
  <run_depend>bitbots_stackmachine</run_depend>
  <run_depend>bitbots_body_behaviour</run_depend>

  <export>
    <bitbots_documentation>
      <status>compiles</status>
      <language>python3</language>
    </bitbots_documentation>
  </export>
</package>
